import java.util.*;
public class minsYear {
	
	public static void main(String args[])
	{
		int min ,day1,day , year;
		int minsperyear = 24*60*365;
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter minutes:  ");
		min=sc.nextInt();
				year= min/minsperyear;
				day1=min/(24*60);
				day=day1%365;
				
		
		System.out.print(year + " years"+ day + "days");
	}
	}
